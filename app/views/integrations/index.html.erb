<% integrations||=@integrations %>
<% pages||=PaginationHelper.pages(integrations,params) %>
<% content_for :window_header do %>
<%= if controller.action_name == "index"
	"Integrations"
	else
		controller.action_name.titleize
end %>
<% end %>

<div id="result">
  <%= render :partial => 'shared/table_toolbar', :locals => {:collection => integrations, :show_csv_export_button => true, :pages => pages} %>
	<div id="table">
    <%= render :partial => "table", :locals => {:integrations => pages.current_page} %>	
	</div>
</div>
