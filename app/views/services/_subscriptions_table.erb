<% subscriptions||=@subscriptions %>
<% table_options||={} %>

<%= redpill_table(subscriptions,table_options) do |table| 
  table.add_column(:name => "consumer id",:width =>"50%") { |o| o.consumer_id}
  table.add_column(:name => "starts at",:width =>"50%") { |o| o.starts_at}
  table.row_class{|r| "disabled"}
end %>

