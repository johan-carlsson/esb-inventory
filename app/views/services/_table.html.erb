<% services||=@services %>
<% table_options||={} %>

<%= redpill_table(services,table_options) do |table| 
	table.add_column(:name => :name,:width => "50%") { |o| link_to(o.name, service_subscriptions_url(o)) }
  table.add_column(:name => :category,:width => "50%",&:category)
  table.row_linker { |o| service_subscriptions_url(o) }
end %>
