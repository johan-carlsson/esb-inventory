<% backends||=@backends%>
<% content_for :window_header do %>
<%= if controller.action_name == "index"
	"backends"
	else
		controller.action_name.titleize
end %>
<% end %>

<div id="result">
  <%= render :partial => 'shared/table_toolbar', :locals => {:collection => backends, :show_csv_export_button => true} %>
	<div id="table">
    <%= render :partial => "table", :locals => {:backends => backends} %>	
	</div>
</div>
