<% remote ||= false %>
<% dom_id = if  @subscription.new_record?
	"new_service"
else
	dom_id(@subscription)
end
%>

<%= redpill_tabular_form([@consumer,@subscription],{:object_name => 'subscription'},{:remote => remote}) do |f| %> 
  <%= f.text_field :service_name, data: {autocomplete_source: Service.order(:name).map(&:name)} %>
	<%= f.submit_and_cancel_buttons %>
<% end %>
