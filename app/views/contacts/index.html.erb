<% contacts||=@contacts%>
<% content_for :window_header do %>
<%= if controller.action_name == "index"
	"contacts"
	else
		controller.action_name.titleize
end %>
<% end %>

<div id="result">
  <%= render :partial => 'shared/table_toolbar', :locals => {:collection => contacts} %>
	<div id="table">
    <%= render :partial => "table", :locals => {:contacts => contacts} %>	
	</div>
</div>
